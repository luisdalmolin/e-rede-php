{
    "name": "luisdalmolin/e-rede-php",
    "description": "A Laravel package to integrates with E-REDE API",
    "license": "MIT",

    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.0"
    },

    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "0.9.*",
        "doctrine/dbal": "~2.3"
    },

    "autoload": {
        "psr-4": {
            "Dalmolin\\ERede\\": "src/"
        }
    },

    "autoload-dev": {
        "classmap": []
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
